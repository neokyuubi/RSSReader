@model RSSReader.Models.AddFeedViewModel

@{
}

<h1 class="mt-5">Add Feed</h1>
<form method="post" action="Add">
    <div class="mb-3">
        <label for="name" class="form-label">Name</label>
        <input type="text" class="form-control" asp-for="Name" id="name" autocomplete="off">
    </div>
    <div class="mb-3">
        <label for="url" class="form-label">Url</label>
        <input type="url" class="form-control" asp-for="Url" autocomplete="off">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>