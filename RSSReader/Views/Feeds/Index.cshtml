@model List<RSSReader.Models.Feed>

@{
}

<h1>Feeds</h1>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Url</th>
            <th>📜</th>
            <th>🎫</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var feed in Model)
        {
            <tr>
                <td>@feed.Name</td>
                <td>@feed.Url</td>
                <td> <a href="Feeds/View/@feed.Id">Details</a> </td>
                <td> <a href="Feeds/Articles/@feed.Id">Articles</a> </td>
            </tr>
        }
    </tbody>
</table>