@model RSSReader.Models.UpdateFeedViewModel

@{
}

<h1 class="mt-5">Edit Feed</h1>
<form method="post" action="View">
    <div class="mb-3">
        <label for="Id" class="form-label">Id</label>
        <input readonly type="text" class="form-control" asp-for="Id" id="Id" aria-describedby="name">
    </div>
    <div class="mb-3">
        <label for="name" class="form-label">Name</label>
        <input type="text" class="form-control" asp-for="Name" id="name" aria-describedby="name">
    </div>
    <div class="mb-3">
        <label for="url" class="form-label">Url</label>
        <input type="url" class="form-control" asp-for="Url">
    </div>
